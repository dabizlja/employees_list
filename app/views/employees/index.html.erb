<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Date of Birth</th>
      <th>Address</th>
      <th>Country</th>
      <th>Bio</th>
      <th>Rating</th>
    </tr>
  </thead>
  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.id %></td>
        <td><%= employee.first_name %></td>
        <td><%= employee.last_name %></td>
        <td><%= employee.email %></td>
        <td><%= employee.date_of_birth %></td>
        <td><%= employee.address %></td>
        <td><%= employee.country %></td>
        <td>
          <ul>
            <% employee.bio.each do |item| %>
              <li><%= item %></li>
            <% end %>
          </ul>
        </td>
        <td><%= employee.rating %></td>
      </tr>
    <% end %>
  </tbody>
</table>